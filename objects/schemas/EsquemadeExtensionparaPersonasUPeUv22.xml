    <definition>
        <xsd:schema xmlns:a="http://prism.evolveum.com/xml/ns/public/annotation-3" xmlns:tns="urn:upeu:midpoint:person" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="urn:upeu:midpoint:person">
            <xsd:import namespace="http://prism.evolveum.com/xml/ns/public/annotation-3"/>
            <xsd:complexType name="DemographicsType">
                <xsd:annotation>
                    <xsd:documentation>Datos demográficos (no nativos): fecha de nacimiento, género ISO5218, país ISO3166-1.</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="birthDate" type="xsd:date">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Fecha de Nacimiento</a:displayName>
                                <a:displayOrder>110</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="gender" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation>ISO/IEC 5218: 1=Masculino, 2=Femenino, 9=No aplica</xsd:documentation>
                            <xsd:appinfo>
                                <a:displayName>Género</a:displayName>
                                <a:displayOrder>120</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="country" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation>País de residencia (ISO 3166-1 alpha-3)</xsd:documentation>
                            <xsd:appinfo>
                                <a:displayName>País de Residencia</a:displayName>
                                <a:displayOrder>130</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="ContactInfoType">
                <xsd:annotation>
                    <xsd:documentation>Solo contacto adicional (email institucional es c:emailAddress).</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="secondaryMail" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Correo Secundario</a:displayName>
                                <a:displayOrder>210</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="phoneNumberAlt" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Teléfono Alternativo</a:displayName>
                                <a:displayOrder>220</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="personalWeb" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Página Personal</a:displayName>
                                <a:displayOrder>230</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="AffiliationDataType">
                <xsd:annotation>
                    <xsd:documentation>Acreditación misionera e idiomas (afiliaciones institucionales vía Org).</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="languageSkills" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Idiomas Dominados</a:displayName>
                                <a:displayOrder>420</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="primaryAffiliation" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Facultad o Área Principal</a:displayName>
                                <a:displayOrder>410</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="academicProgram" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Programa Académico / Carrera</a:displayName>
                                <a:displayOrder>515</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="AcademicStatusType">
                <xsd:annotation>
                    <xsd:documentation>Estado académico (estructura académica vía Org: Facultad/Programa).</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="studentCycle" type="xsd:int">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Ciclo Académico</a:displayName>
                                <a:displayOrder>510</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="alumniStatus" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Estado de Egreso</a:displayName>
                                <a:displayOrder>520</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="FederatedIdentityType">
                <xsd:annotation>
                    <xsd:documentation>Identidad federada complementaria.</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="orcid" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>ORCID</a:displayName>
                                <a:displayOrder>610</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="EmploymentDataType">
                <xsd:annotation>
                    <xsd:documentation>Fechas laborales (c:employeeType y c:title son nativos; estructura por Org).</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="hireDate" type="xsd:date">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Fecha de Ingreso</a:displayName>
                                <a:displayOrder>310</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="terminationDate" type="xsd:date">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Fecha de Cese</a:displayName>
                                <a:displayOrder>320</a:displayOrder>
                                <a:indexed>false</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="UniqueIdentifiersType">
                <xsd:annotation>
                    <xsd:documentation>Identificadores complementarios (employeeNumber es nativo).</xsd:documentation>
                    <xsd:appinfo>
                        <a:extension ref="c:UserType"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="taxId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>DNI/CE</a:displayName>
                                <a:displayOrder>710</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="institutionalIdCard" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>ID Institucional</a:displayName>
                                <a:displayOrder>720</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="universityIdCard" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>Carnet Universitario</a:displayName>
                                <a:displayOrder>730</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="externalSystemId" type="xsd:string">
                        <xsd:annotation>
                            <xsd:appinfo>
                                <a:displayName>ID Sistema Externo</a:displayName>
                                <a:displayOrder>740</a:displayOrder>
                                <a:indexed>true</a:indexed>
                            </xsd:appinfo>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:schema>
    </definition>
</schema>
